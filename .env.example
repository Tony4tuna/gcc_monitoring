# GCC Monitoring System - Environment Configuration
# Copy this file to .env and set your production values

# Admin account credentials
ADMIN_EMAIL=admin
ADMIN_PASSWORD=your_secure_password_here

# Storage secret (generate with: python -c "import secrets; print(secrets.token_urlsafe(32))")
STORAGE_SECRET=your_secret_key_here

# Server configuration
HOST=0.0.0.0
PORT=8080

# Development only: Enable test data generator (set to 1 to enable)
# ENABLE_TEST_DATA=0
